{
  "name": "alien-dom",
  "version": "0.6.1",
  "description": "Enhanced DOM APIs mixed with jsx-dom",
  "main": "index.js",
  "types": "index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alloc/alien-dom.git"
  },
  "files": [
    "dist",
    "types",
    "*.d.ts",
    "*.js"
  ],
  "sideEffects": [
    "global.js"
  ],
  "keywords": [
    "jsx-dom",
    "dom",
    "jquery",
    "jsx",
    "element"
  ],
  "author": "Alec Larson",
  "license": "MIT",
  "prettier": "@alloc/prettier-config",
  "scripts": {
    "build": "rimraf dist && cd src && tsup-node --treeshake smallest --define.DEV false",
    "dev": "cd src && tsup-node --watch --sourcemap --define.DEV true"
  },
  "dependencies": {
    "@alloc/types": "^2.3.0",
    "@preact/signals-core": "^1.3.0",
    "csstype": "^3.1.2"
  },
  "devDependencies": {
    "@alloc/fast-rimraf": "^1.0.8",
    "@alloc/is": "^3.1.2",
    "@alloc/prettier-config": "^1.0.0",
    "ava": "^5.2.0",
    "linear-color": "^1.2.0",
    "prettier": "^2.8.7",
    "tsup": "^6.7.0",
    "tsx": "^3.12.7",
    "typescript": "^4.9.5"
  },
  "ava": {
    "files": [
      "**/*.test.*"
    ],
    "extensions": {
      "ts": "module"
    },
    "nodeArguments": [
      "--loader=tsx"
    ]
  }
}
