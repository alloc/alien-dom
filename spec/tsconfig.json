{
  "include": ["./"],
  "compilerOptions": {
    "esModuleInterop": true,
    "jsx": "react-jsx",
    "jsxImportSource": "./jsx-runtime",
    "lib": ["dom", "esnext"],
    "module": "esnext",
    "moduleResolution": "node",
    "noEmit": true,
    "outDir": "../dist",
    "target": "esnext",
    "skipLibCheck": true,
    "strict": true,
    "types": []
  }
}
